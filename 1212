qname_extract <- function(list, class, path=getwd(), prefix='') {
  for(i in seq_along(list)) {
    frag2 <- list[[i]]
    f2_di <- frag2$V4[frag2$class %in% class]
    if(isFALSE(grepl(path, pattern = '&/'))) {
      path <- paste(path,'/', sep = '')}
    write.table(x = f2_di, file = paste(path, prefix, names(list[i]), '_qname.txt', sep = '')
                , quote = F, row.names = F, col.names = F)
  }
}

qname_extract(list = frag, class = 'DiRNA', prefix = 'MHV')
